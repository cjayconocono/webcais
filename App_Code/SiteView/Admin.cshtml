@helper showError(string msg)
{
    <script type="text/javascript">
             window.onload = function () {
                 swal("\"" + msg + "\"");
             };
    </script>
}

@helper adminChecker()
{
if (Session["user_level"] == null || Session["user_level"] != "Administrator")
{
    Response.Redirect("~/Admin/");
}
}

@helper adminIsLogged()
{
if (Session["user_level"] != null && Session["user_level"] == "Administrator")
{
    Response.Redirect("~/Admin/dashboard/");
}
}