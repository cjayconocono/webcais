@helper showError(string msg) {
    <script type="text/javascript">
             window.onload = function () {
                 swal("\"" + msg + "\"");
             };
    </script>
}

@helper profChecker() {
    if (Session["user_level"] == null || Session["user_level"] != "Professor")
    {
        Response.Redirect("~/Professors/Login");
    }
}

@helper profIsLogged() { 
    if (Session["user_level"] != null && Session["user_level"] == "Professor")
    {
        Response.Redirect("~/Professors/Index");
    }
}