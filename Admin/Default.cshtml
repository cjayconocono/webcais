@{ 
    if (IsPost)
    {
        RTUDB conn = new RTUDB();
        conn.bind("1", Request["pass"]);
        string val = null;
        val = conn.SelectSingleValue("Select * from Administrator where Password = @1");
        if (val == null)
        {
            //error message like invalid password
            Response.Write(@"<script type=""text/javascript"" language=""javascript"">alert(""Invalid password"");</script>");
        }
        else
        {
            Session["user_level"] = "admin";
            Response.Redirect("~/Admin/Admin");
        }
    }
}
